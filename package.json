  "scripts": {
    "ng": "ng",
    "start": "set NODE_ENV=development && concurrently \"ng serve --proxy-config proxy.conf.json\" \"nodemon --watch server server-start.js\" ",
    "start-server": "set NODE_ENV=development && nodemon --watch server server-start.js",
    "start-prod": "set NODE_ENV=production && node server-start.js",
    "start-bg": "node scheduler.js",
    "w-flow-start": "node schedule start",
    "w-flow-stop": "node schedule stop",
    "build": "npm run clean && ng build",
    "build:prod": "npm run clean && ng build --prod --source-map=false --vendor-chunk=true --extract-css=true --build-optimizer=true --aot=true",
    "test": "mocha --timeout 10000",
    "clean": "rimraf -- dist",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install"
